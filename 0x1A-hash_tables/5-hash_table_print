#include "hash_tables.h"

void hash_table_print(const hash_table_t *ht)
{
	hash_node_t *temp;
	unsigned long int i;

	printf("%c", 123);
	for (i = 0; i < ht->size; i++)
	{
		if (ht->array[i] != NULL)
		{
			temp = ht->array[i];
			while (temp)
			{
				/*if ((i == (ht->size) - 1) && temp->next == NULL)*/
				{
					printf("'%s':'%s', ", temp->key, temp->value);
					temp = temp->next;
				}
				/*else
				{
					printf("'%s':'%s', ", temp->key, temp->value);
					temp = temp->next;
				}*/
			}
		}
	}
}
